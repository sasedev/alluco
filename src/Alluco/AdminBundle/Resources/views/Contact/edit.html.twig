{% extends 'AllucoAdminBundle::template.html.twig' %}
{%block breadcrumb%}
<ol class="breadcrumb">
    <li><a href="{{ path('_admin_homepage') }}">{{ faIco('dashboard') }} &nbsp; {{'Breadcrumb.admin.home'|trans|raw}}</a></li>
    <li><a href="{{ path('_admin_contact_list') }}">{{faIco('envelope-o')}} &nbsp; {{'Breadcrumb.admin.contact.list'|trans|raw}}</a></li>
    <li class="active"><a href="{{ path('_admin_contact_editGet', {'id': contact.id}) }}">{{glyphIco('edit')}} &nbsp; {{'Breadcrumb.admin.contact.edit'|trans({'%contact%': contact.id})|raw}}</a></li>
</ol>
{%endblock breadcrumb%}
{%block content%}
<div class="row">
    <div class="col-lg-12">
        <div class="well well-sm text-right">
            <a href="{{ path('_admin_contact_list') }}" class="btn btn-primary">{{ faIco('th') }} {{ 'Action.list'|trans }}</a>
        </div>
        <div class="box box-primary">
            <div class="box-header"><h3 class="box-title">{{'Contact.tlabel.overview'|trans}}</h3></div>
            <div class="box-body">
                <div class="row">
                    <div class="col-lg-3 text-right">{{'Contact.id'|trans|raw}} :</div>
                    <div class="col-lg-9">{{contact.id}}&nbsp;</div>
                    <div class="col-lg-12">&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.firstName'|trans|raw}} :</div>
                    <div class="col-lg-9">{{contact.firstName}}&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.lastName'|trans|raw}} :</div>
                    <div class="col-lg-9">{{contact.lastName}}&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.email'|trans|raw}} :</div>
                    <div class="col-lg-9">{{ contact.email }}&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.job'|trans|raw}} :</div>
                    <div class="col-lg-9">{{ contact.job.name }}&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.company'|trans|raw}} :</div>
                    <div class="col-lg-9">{{ contact.company }}&nbsp;</div>
                    <div class="col-lg-12">&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.subject'|trans|raw}} :</div>
                    <div class="col-lg-9">{{ contact.subject }}&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.msg'|trans|raw}} :</div>
                    <div class="col-lg-9"><br><br>{{ contact.msg|raw }}&nbsp;</div>
                    <div class="col-lg-12">&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.dtCrea'|trans|raw}} :</div>
                    <div class="col-lg-9">{{contact.dtCrea|localeDate('medium')}} {{'_At'|trans}} {{contact.dtCrea|localeDate('none', 'short')}}&nbsp;</div>
                    <div class="col-lg-3 text-right">{{'Contact.dtUpdate'|trans|raw}} :</div>
                    <div class="col-lg-9">{{contact.dtUpdate|localeDate('medium')}} {{'_At'|trans}} {{contact.dtUpdate|localeDate('none', 'short')}}&nbsp;</div>
                    <br>
                </div>
            </div>
        </div>
    </div>
</div>
{%endblock content%}
