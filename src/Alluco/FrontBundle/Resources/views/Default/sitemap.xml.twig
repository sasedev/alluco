<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ url('_front_homepage') }}</loc>
    <lastmod>{{ ROOTPage.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>1</priority>
  </url>
  <url>
    <loc>{{ url('_front_certificats') }}</loc>
    <lastmod>{{ certificatsPage.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>1</priority>
  </url>
  <url>
    <loc>{{ url('_front_partenaires') }}</loc>
    <lastmod>{{ partenairesPage.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>1</priority>
  </url>
  <url>
    <loc>{{ url('_front_contact') }}</loc>
    <lastmod>{{ contactPage.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>never</changefreq>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>{{ url('_front_sitenews') }}</loc>
    <lastmod>{{ newsPage.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>1</priority>
  </url>
  {% for sitenew in news %}
  <url>
    <loc>{{ url('_front_sitenew', {'pageUrl': sitenew.pageUrl}) }}</loc>
    <lastmod>{{ sitenew.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  {% endfor %}
  <url>
    <loc>{{ url('_front_productgroups') }}</loc>
    <lastmod>{{ prodsPage.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>1</priority>
  </url>
  {% for prod in groups %}
  <url>
    <loc>{{ url('_front_products', {'group': prod.pageUrl}) }}</loc>
    <lastmod>{{ prod.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.9</priority>
  </url>
  {% endfor %}
  <url>
    <loc>{{ url('_front_docs') }}</loc>
    <lastmod>{{ docsPage.dtUpdate|date('Y-m-d') }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.7</priority>
  </url>
</urlset>